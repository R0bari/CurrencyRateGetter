    <ItemGroup>
      <Reference Include="DataBase, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null">
        <HintPath>..\DataBase\bin\Debug\net6.0\DataBase.dll</HintPath>
      </Reference>
    </ItemGroup>
